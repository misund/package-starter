{
  "name": "package-starter",
  "version": "0.1.2",
  "description": "Start an npm package.",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "lint": "eslint --max-warnings=0 src",
    "mocha": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register",
    "prepublish": "yarn build && yarn test",
    "postpublish": "git push --tags",
    "test": "yarn lint && yarn mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/misund/get-relative-luminance.git"
  },
  "keywords": [
    "init",
    "package starter"
  ],
  "author": "Just Thomas Hiorth Misund",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/misund/package-starter/issues"
  },
  "homepage": "https://github.com/misund/package-starter#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "eslint": "5.14.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "mocha": "5.2.0"
  }
}
